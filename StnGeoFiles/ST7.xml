<Station><!--50MHz Standard Station. Four 50MHz Create LPDA's pointing down in pairs-->
  <Antennas><!--This will be a vector of AntennaPlacement's-->

    <Antenna><!--This will be an AntennaPlacemtent struct-->
      <AntType>4</AntType>  <!--Type 4 is 50MHz Create LPDA-->
      <position>  <!--Position of antenna relative to station center-->
	<value>3.</value>  <!--x-->
	<value>0.</value>  <!--y-->
	<value>0.</value>  <!--y-->
      </position>
      <n_boresight>  <!--Direction the antenna is pointed.  Forward direction for LPDA's. Along axis for Dipoles. MUST BE NORMAL VECTOR-->
	<value>0.</value>  <!--x-->
	<value>0.</value>  <!--y-->
	<value>-1.</value> <!--z-->
      </n_boresight>
      <n_eplane>  <!--Down the tines for LPDA. Irrelevant for Dipoles due to axial symmetry. MUST BE NORMAL VECTOR-->
	<value>0.</value>  <!--x-->
	<value>1.</value>  <!--y-->
	<value>0.</value>  <!--z-->
      </n_eplane>
    </Antenna>

    <Antenna>
      <AntType>4</AntType>
      <position>
	<value>0.</value>
	<value>3.</value>
	<value>0.</value>
      </position>
      <n_boresight>
	<value>0.</value>
	<value>0.</value>
	<value>-1.</value>
      </n_boresight>
      <n_eplane>
	<value>-1.</value>
	<value>0.</value>
	<value>0.</value>
      </n_eplane>
    </Antenna>

    <Antenna>
      <AntType>4</AntType>
      <position>
	<value>-3.</value>
	<value>0.</value>
	<value>0.</value>
      </position>
      <n_boresight>
	<value>0.</value>
	<value>0.</value>
	<value>-1.</value>
      </n_boresight>
      <n_eplane>
	<value>0.</value>
	<value>-1.</value>
	<value>0.</value>
      </n_eplane>
    </Antenna>

    <Antenna>
      <AntType>4</AntType>
      <position>
	<value>0.</value>
	<value>-3.</value>
	<value>0.</value>
      </position>
      <n_boresight>
	<value>0.</value>
	<value>0.</value>
	<value>-1.</value>
      </n_boresight>
      <n_eplane>
	<value>1.</value>
	<value>0.</value>
	<value>0.</value>
      </n_eplane>
    </Antenna>

  </Antennas>
</Station>
